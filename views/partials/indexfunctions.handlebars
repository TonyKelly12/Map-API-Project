<!-- google maps api-->
<script type="text/javascript">

    $(function () {
        $('#get-favList').on('click', function (event) {
            event.preventDefault();
            var favList = []; 
            $.ajax({
                url: '/index',
                method: 'GET',
                contentType: 'application/json',
                success: function (response) {
                    response.favLocation.forEach(function(fav){
                        favList.push(fav);
                    }); 
                               
                    console.log(favList[0]);
                }

            });
        });
    });
</script>

